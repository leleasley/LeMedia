# Local/staging web overrides (used by lemedia-web-staging only)
#
# Copy to .env.staging and adjust values:
#   cp .env.staging.example .env.staging

# Set to your server LAN IP + staging port so CSRF/auth cookies work correctly for IP testing.
APP_BASE_URL=http://192.168.1.50:3011
INTERNAL_APP_BASE_URL=http://lemedia-web-staging:3010

# Safety: keep background jobs and notifications off in staging.
JOB_SCHEDULER_ENABLED=0
NOTIFICATIONS_ENABLED=false

# Optional bind settings for docker-compose.staging.yml
STAGING_BIND_IP=0.0.0.0
STAGING_PORT=3011
